{
  "manifest_version": 3,
  "name": "RYM Artist & Release Checker",
  "version": "1.0",
  "description": "Check if an artist and release are already stored",
  "permissions": [
    "activeTab",
    "http://localhost:3000/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://rateyourmusic.com/release/*"],
      "js": ["content.js"]
    }
  ]
}
